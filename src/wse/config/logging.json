{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(ascname)s - %(name)s - %(levelname)s - %(messages)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "format": "default",
      "stream": "ext://sys.stdout"
    },
    "file": {
      "class": "logging.FileHandler",
      "level": "DEBUG",
      "formatter": "default",
      "filename": "logs/wse.log",
      "mode": "a"
    }
  },
  "loggers": {
    "features": {
      "level": "DEBUG",
      "handlers": ["file"],
      "propagate": true
    }
  },
  "root": {
    "level": "DEBUG",
    "loggers": ["console", "file"]
  }
}